    "input_list=[1,1,4,300,400,900]\n",
    "flag = 0\n",
    "i = 1\n",
    "while i < len(input_list): \n",
    "    if(input_list[i] <= input_list[i - 1]): \n",
    "        flag = 1\n",
    "    i += 1\n",
    "      \n",
    "# printing result \n",
    "if (not flag) : \n",
    "    print (\"Yes\") \n",
    "else : \n",
    "    print (\"No\") "
